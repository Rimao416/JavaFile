package TP2;
import TP2.Point;
public abstract class Forme {

		Point centre;
		abstract double perimetre();
		abstract double surface();
		abstract void   affiche();
	
	public Forme(Point P)

	{
		centre = P;
	}
	public Forme()
	{
		
	}
	
		public void deplacer(int x, int y)
	{
		centre.deplacer(x,y);
	}
	
	
	}
	


